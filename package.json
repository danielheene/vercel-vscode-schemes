{
  "name": "zeit-co-vscode-schemas",
  "displayName": "vercel.com Schemas",
  "description": "Add support for JSON Schemas from vercel.com files",
  "version": "0.1.1",
  "keywords": [
    "zeit",
    "now",
    "now.sh",
    "vercel.com",
    "vercel",
    "zeit.co",
    "schemas",
    "nextjs",
    "config"
  ],
  "publisher": "ph1p",
  "engines": {
    "vscode": "^1.42.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/index.js",
  "scripts": {
    "packages": "vsce package",
    "publish": "vsce publish",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc  -p ./",
    "lint": "tslint -p ./",
    "watch": "tsc -watch -p ./"
  },
  "contributes": {
    "jsonValidation": [
      {
        "fileMatch": [
          "now.json",
          "vercel.json"
        ],
        "url": "./schemas/vercel.json"
      },
      {
        "fileMatch": [
          "*.now/config.jsonn",
          "*.vercel/config.json"
        ],
        "url": "./schemas/config.json"
      }
    ]
  },
  "activationEvents": [
    "*"
  ],
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#ffffff",
    "theme": "light"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ph1p/zeit-co-vscode-schemas/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ph1p/zeit-co-vscode-schemas.git"
  },
  "devDependencies": {
    "@types/node": "^14.0.26",
    "@types/vscode": "^1.47.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.7"
  }
}
