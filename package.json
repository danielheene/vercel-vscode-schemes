{
  "name": "zeit-co-vscode-schemas",
  "displayName": "zeit.co Schemas",
  "description": "Add support for JSON Schemas from zeit.co files",
  "version": "0.0.7",
  "keywords": [
    "zeit",
    "now",
    "now.sh",
    "zeit.co",
    "schemas",
    "nextjs",
    "config"
  ],
  "publisher": "ph1p",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/index.js",
  "scripts": {
    "packages": "vsce package",
    "publish": "vsce publish",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc  -p ./",
    "lint": "tslint -p ./",
    "watch": "tsc -watch -p ./"
  },
  "contributes": {
    "jsonValidation": [
      {
        "fileMatch": "now.json",
        "url": "./schemas/now.json"
      },
      {
        "fileMatch": "*.now/config.json",
        "url": "./schemas/config.json"
      }
    ]
  },
  "activationEvents": [
    "*"
  ],
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#ffffff",
    "theme": "light"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ph1p/zeit-co-vscode-schemas/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ph1p/zeit-co-vscode-schemas.git"
  },
  "devDependencies": {
    "@types/node": "^13.7.1",
    "@types/vscode": "^1.42.0",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  }
}
